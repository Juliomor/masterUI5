<mvc:View controllerName="alight.sapui5.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core" displayBlock="true">
    <Page title = "{i18n>appTitle}">
        <headerContent>
            <Button id = "helloDialogHeader" press=".onOpenDialogHeader" class = "sapUiSmallMarginEnd" icon="sap-icon://action"/>
        </headerContent>
        <content>
            <mvc:XMLView viewName="alight.sapui5.view.HelloPanel"/>
            <mvc:XMLView viewName="alight.sapui5.view.InvoicesList"/>
        </content>
    </Page>
</mvc:View>